@using System.Runtime.InteropServices.ComTypes
@model IEnumerable<A2ZPortal.Core.ViewModel.UserManagement.EmployeeModel>

<div id="example1_wrapper" class="dataTables_wrapper form-inline dt-bootstrap">
    <div class="row">
        <div class="col-sm-12">
            <table id="example1" class="table table-bordered table-striped dataTable" role="grid" aria-describedby="example1_info">
                <thead>
                    <tr role="row">
                        <th>Emp Code </th>
                        <th>Employee Name</th>
                        <th>Phone</th>
                        <th>Email </th>
                        <th>Role </th>
                        <th>User Name </th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var data in Model)
                    {
                        <tr>

                            <td>@data.EmpCode</td>
                            <td>@data.EmployeeName</td>
                            <td>@data.Phone</td>
                            <td>@data.Email</td>
                            <td>@data.Role</td>
                            <td>@data.UserName</td>
                            <td>
                                <button class="btn btn-primary">
                                    <i class="fa fa-edit" onclick="Edit(@data.EmployeeId)"></i>
                                </button>
                                <button class="btn btn-danger" onclick="Delete(@data.EmployeeId)">
                                    <i class="fa fa-trash"></i>
                                </button>
                            </td>
                        </tr>
                    }
                </tbody>

            </table>
        </div>
    </div>
</div>
<script>
    $(function () {
        $('#example1').DataTable();

    })
</script>