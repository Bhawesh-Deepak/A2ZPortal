<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] </title>
    <link rel="stylesheet" href="~/wp-contents/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/wp-contents/bower_components/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/wp-contents/bower_components/Ionicons/css/ionicons.min.css">
    <link rel="stylesheet" href="~/wp-contents/dist/css/AdminLTE.min.css">
    <link rel="stylesheet" href="~/wp-contents/dist/css/skins/_all-skins.min.css">
    <link rel="stylesheet" href="~/wp-contents/bower_components/morris.js/morris.css">
    <link rel="stylesheet" href="~/wp-contents/bower_components/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="~/wp-contents/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css">
    <link rel="stylesheet" href="~/wp-contents/bower_components/bootstrap-daterangepicker/daterangepicker.css">
    <link rel="stylesheet" href="~/wp-contents/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-ajax-unobtrusive@3.2.6/dist/jquery.unobtrusive-ajax.min.js"
            integrity="sha256-PAC000yuHt78nszJ2RO0OiDMu/uLzPLRlYTk8J3AO10="
            crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/wp-contents/bower_components/datatables.net-bs/css/dataTables.bootstrap.min.css">
    <script src="~/wp-contents/bower_components/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/wp-contents/bower_components/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <link href="~/AlertifyJs/build/css/themes/bootstrap.css" rel="stylesheet" />
    <link href="~/AlertifyJs/build/css/alertify.css" rel="stylesheet" />
    <script src="~/AlertifyJs/build/alertify.js"></script>
    <link rel="shortcut icon" href="/images/logo.png" type="image/x-icon">
    <style>
        .input-validation-error {
            border-color: orangered;
        }

        .text-danger {
            color: orangered !important;
        }
    </style>
    <style>
        .loading {
            position: revert;
        }

            .loading:before {
                content: "";
                display: block;
                position: absolute;
                z-index: 9;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                background: rgba(255, 2555, 255,0.6) url(../images/loading.gif) no-repeat center 35vh;
                background-size: 85px;
            }

        em {
            color: orangered
        }

        .text-danger {
            color: orangered !important
        }
    </style>
</head>
<body class="hold-transition skin-yellow sidebar-mini">
    <div class="wrapper">

        <header class="main-header">
            <!-- Logo -->
            <a asp-action="Index" asp-controller="Home" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>A</b>2Z</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>A2Z </b> Property</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">

                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/wp-contents/dist/img/user2-160x160.jpg" class="user-image" alt="User Image">
                                <span class="hidden-xs">Admin</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="~/wp-contents/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">

                                    <p>
                                        Admin - Web Developer
                                        <small>Member since Nov. 2012</small>
                                    </p>
                                </li>
                                <li class="user-footer">
                                    <div class="pull-right">
                                        <a asp-action="Index" asp-controller="Authenticate" class="btn btn-default btn-flat">
                                            <i class="fa fa-logout">Log out</i>
                                        </a>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <aside class="main-sidebar">
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/wp-contents/dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>Admin</p>
                        <a asp-action="Index" asp-controller="Home"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>
                <!-- search form -->
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" id="myInputSearch" onkeyup="FilterSearch()" name="q" autocomplete="off" class="form-control" placeholder="Search Menu...">
                        <span class="input-group-btn">
                            <button type="button" onclick="FilterSearch()" name="search" id="search-btn" class="btn btn-flat">
                                <i class="fa fa-search"></i>
                            </button>
                        </span>
                    </div>
                </form>
                @await Component.InvokeAsync("MenuSubMenu")
            </section>
        </aside>



        @RenderBody()
        @await Html.PartialAsync("~/Views/Shared/A2ZCommonModalPopUp.cshtml")
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                 
            </div>
            <strong>Copyright &copy; @DateTime.Now.Year <a target="_blank" href="http://klbs.in">KLBS Softech Pvt. Ltd.</a></strong> All rights
            reserved.
        </footer>


        <div class="control-sidebar-bg"></div>
    </div>

    @*<script src="~/wp-contents/bower_components/jquery/dist/jquery.min.js"></script>
        <script src="~/wp-contents/bower_components/jquery-ui/jquery-ui.min.js"></script>*@
    <script src="~/wp-contents/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/wp-contents/bower_components/raphael/raphael.min.js"></script>
    <script src="~/wp-contents/bower_components/morris.js/morris.min.js"></script>
    <script src="~/wp-contents/bower_components/jquery-sparkline/dist/jquery.sparkline.min.js"></script>
    <script src="~/wp-contents/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/wp-contents/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/wp-contents/bower_components/jquery-knob/dist/jquery.knob.min.js"></script>
    <script src="~/wp-contents/bower_components/moment/min/moment.min.js"></script>
    <script src="~/wp-contents/bower_components/bootstrap-daterangepicker/daterangepicker.js"></script>
    <script src="~/wp-contents/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js"></script>
    <script src="~/wp-contents/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
    <script src="~/wp-contents/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/wp-contents/bower_components/fastclick/lib/fastclick.js"></script>
    <script src="~/wp-contents/dist/js/adminlte.min.js"></script>
    <script src="~/wp-contents/dist/js/pages/dashboard.js"></script>
    <script src="~/wp-contents/dist/js/demo.js"></script>
    <script src="~/CustomJavaScript/Layout.js"></script>
</body>
</html>
