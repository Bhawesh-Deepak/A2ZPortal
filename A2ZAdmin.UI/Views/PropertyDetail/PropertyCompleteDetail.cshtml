@model A2ZPortal.Core.ViewModel.PropertyList.PropertyDetailListVm
@{
    var images = Model.PropertyImages.ToList();
    var aminities = Model.Amenities.ToList();

}

<style>
    .users-list > li img {
        border-radius: 2%;
        max-width: 100%;
        height: auto;
    }
</style>

<div class="row">
    <div class="col-md-12">
        <div class="box box-success">
            <div class="box-header with-border">
                <h3 class="box-title">Property Complete Detail</h3>

            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <div class="box-body">

                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PropertyName">Property Name</label>
                                <input type="text" asp-for="PropertyName" class="form-control" readonly />

                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PropertyDescription">Property Description</label>
                                <input type="text" asp-for="PropertyDescription" class="form-control" readonly />

                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <input asp-for="Id" type="hidden" />
                                <label asp-for="LocationName">Location</label>
                                <input type="text" asp-for="LocationName" class="form-control" readonly />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="SubLocationName">Sub Location</label>
                                <input type="text" asp-for="SubLocationName" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="BedRooms">Bed Room</label>
                                <input type="text" asp-for="BedRooms" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="BathRooms">Bath Room</label>
                                <input type="text" asp-for="BedRooms" class="form-control" readonly />
                            </div>
                        </div>
                    </div>
                    <div class="row">

                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="RoomName">Additional Room</label>
                                <input type="text" asp-for="RoomName" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="FurnishingStatus">Furnishing Status</label>
                                <input type="text" asp-for="FurnishingStatus" class="form-control" readonly />
                            </div>
                        </div>

                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="Age">Age Of Property</label>
                                <input type="text" asp-for="Age" class="form-control" readonly />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="TotalParking">No. Of Parking</label>
                                <input type="text" asp-for="Age" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="Facing">View Facing</label>
                                <input type="text" asp-for="Facing" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="DefiningLocation">Defining Location</label>
                                <input type="text" asp-for="DefiningLocation" class="form-control" readonly />
                            </div>
                        </div>
                    </div>
                    <div class="row">

                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="PriceName">Explaining Price</label>
                                <input type="text" asp-for="PriceName" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="ExplaningName">Explaining The Property</label>
                                <input type="text" asp-for="PriceName" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="DefiningStructure">Defining Size and Structure</label>
                                <input type="text" asp-for="DefiningStructure" class="form-control" readonly />
                            </div>
                        </div>
                    </div>
                    <div class="row">

                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="SuitableName">Suitable For</label>
                                <input type="text" asp-for="SuitableName" class="form-control" readonly />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="FloorNumber">Floor Number</label>
                                <input type="text" asp-for="FloorNumber" class="form-control" placeholder="Floor Number" />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="TowerBlock">Tower Block</label>
                                <input type="text" asp-for="TowerBlock" class="form-control" placeholder="Tower Block" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="UnitNumber">Unit Number</label>
                                <input type="text" asp-for="UnitNumber" class="form-control" placeholder="Floor Number" />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="TarkeshiNumber">Trakheesi Number</label>
                                <input type="text" asp-for="TarkeshiNumber" class="form-control" placeholder="Trakheesi Number" />
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label asp-for="SpaceType">Space Type</label>
                                <input type="text" asp-for="SpaceType" class="form-control" placeholder="Space Type" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="box box-success">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Amenities  Details</h3>
                                </div>
                                <!-- /.box-header -->
                                <div class="box-body">
                                    <ul class="users-list clearfix">
                                        @foreach (var item in aminities)
                                        {
                                            <li>
                                                <span class="users-list-date">Today</span>

                                            </li>
                                        }
                                    </ul>

                                </div>
                                <!-- /.box-body -->
                            </div>
                            <!-- /.box -->
                        </div>
                        <div class="col-md-6">
                            <div class="box box-warning">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Images</h3>
                                </div>
                                <!-- /.box-header -->
                                <div class="box-body">
                                    <div class="box-body no-padding">
                                        <ul class="users-list clearfix">
                                            @foreach (var item in images)
                                            {
                                                <li>
                                                    <img src="@item.ImagePath" alt="User Image">

                                                </li>
                                            }
                                        </ul>

                                        <!-- /.users-list -->
                                    </div>
                                </div>
                                <!-- /.box-body -->
                            </div>
                            <!-- /.box -->
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="box box-success">
                                <div class="box-header with-border">
                                    <h3 class="box-title">Loaction</h3>
                                </div>
                                <!-- /.box-header -->
                                <div class="box-body">
                                    <div id="googleMap" style="width:100%;height:400px;"></div>


                                </div>
                                <!-- /.box-body -->
                            </div>
                            <!-- /.box -->
                        </div>

                    </div>


                </div>


            </div>
            <!-- /.box-body -->
        </div>
    </div>
</div>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBKGAp4SyPy1qnUqz5gMygSOzO8Qm2kNvY&callback=myMap"></script>

<script>
    function myMap() {
        var long = '@Model.longitude';
        var lat = '@Model.Latitude'
        debugger;
        var mapProp = {
            center: new google.maps.LatLng(lat, long),
            zoom: 5,
        };
        var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);
    }
</script>

