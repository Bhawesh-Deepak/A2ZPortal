@{
    ViewData["Title"] = "Area Measurement Master";
}
<div class="content-wrapper">

    <section class="content-header">
        <h1>
            @ViewData["Header"]
            <small> @ViewData["Header"] Details</small>
        </h1>
    </section>
    <section class="content" id="divContent">
        <div class="row">
            <div class="col-md-8">
                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title">Area Measurement Master Details</h3>
                        <button class="btn btn-primary pull-right" onclick="Create()">
                            <i class="fa fa-plus">&nbsp;Add</i>
                        </button>
                    </div>

                    <div class="box-body">
                        <div id="divData"></div>
                    </div>
                </div>
            </div>

            <div class="col-md-3">
                @await Component.InvokeAsync("Filter")
            </div>
        </div>
    </section>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

<environment include="Production">
    <script src="~/CustomJavaScript/AreaMeasurement.js"></script>
</environment>
<environment include="Development">
    <script src="~/CustomJavaScript/AreaMeasurement.js"></script>
</environment>

