<!DOCTYPE html>
<html lang="en">
<head>
    <title>A2Z Properties @ViewData["Title"]</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/A2Z-wp-contents/css/bootstrap.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/A2Z-wp-contents/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/A2Z-wp-contents/css/owl.theme.default.min.css">
    <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/A2Z-wp-contents/css/style.css">
    <link rel="stylesheet" href="~/A2Z-wp-contents/css/lightbox.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.js"></script>
    <script src="https://code.jquery.com/ui/1.13.0/jquery-ui.js"></script>
    <script src="~/A2Z-wp-contents/js/owl.carousel.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
    <script src="~/A2Z-wp-contents/js/lightbox.js"></script>
    
</head>
 
<body>
    <header>
        <div class="top-bar">
            <div class="container">
                <div class="row">
                    <div class="col-lg-2 py-2">
                        <a href="tel:+971 50 109 9963"><i class="fa fa-phone"></i> +971 50 109 9963</a>
                    </div>
                    <div class="col-lg-7 py-2">
                        <a href="mailto:info@a2zproperties.ae"><i class="fa fa-envelope"></i> info@a2zproperties.ae</a>
                    </div>
                    <div class="col-lg-3 text-lg-right mb-md-0 mb-3">
                        <a href="https://www.facebook.com/a2zproperties.ae" target="_blank"><i class="fa fa-facebook fa-2x"></i></a>
                        <a href="https://www.youtube.com/channel/UCxRQ1ukVeC7xKgJPBK5fPFg" target="_blank"><i class="fa fa-youtube fa-2x"></i></a>
                        <a href="https://www.linkedin.com/company/a2z-propertiesllc/" target="_blank"><i class="fa fa-linkedin fa-2x"></i></a>
                        <a href="https://www.instagram.com/a2zprop/" target="_blank"><i class="fa fa-instagram fa-2x"></i></a>
                        <a href="#" onclick="SubmitContact()"><i class="fa fa-lock fa-2x"></i> Login</a>
                    </div>
                </div>
            </div>
        </div>
        <nav class="navbar navbar-expand-md" id="navbar">
            <div class="container">
                <a class="navbar-brand" asp-action="Index" asp-controller="Home"><img src="~/A2Z-wp-contents/images/logo.png" alt="" class="img-fluid"></a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar" aria-expanded="false"><span class="navbar-toggler-icon"></span></button>
                <div class="collapse navbar-collapse" id="collapsibleNavbar">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <a class="nav-link" asp-action="Index" asp-controller="AboutUs">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-action="Index" asp-controller="Sale">For Sale</a>
                        </li>
                        @*<li class="nav-item">
                                <a class="nav-link" asp-action="Index" asp-controller="Rental">For Rent</a>
                            </li>*@
                        <li class="nav-item">
                            <a class="nav-link" asp-action="Index" asp-controller="Projects">New Projects</a>
                        </li>
                        @*<li class="nav-item">
                                <a class="nav-link" href="#">360 VR</a>
                            </li>*@
                        <li class="nav-item">
                            <a class="nav-link" asp-action="Index" asp-controller="UpcomingProject">New Launch</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-action="Index" asp-controller="ContactUs">Contact Us</a>
                        </li>

                    </ul>
                </div>
            </div>
        </nav>
    </header>
    @RenderBody()
    
    @*@await Html.PartialAsync("~/Views/Shared/_LoginPopUp.cshtml")*@

    
    <script type="text/javascript">
        (function () {
            var options = {
                whatsapp: "+971501099963", // WhatsApp number
                call_to_action: "Hello, how may we help you?", // Call to action
                position: "right", // Position may be 'right' or 'left'
            };
            var proto = document.location.protocol, host = "getbutton.io", url = proto + "//static." + host;
            var s = document.createElement('script'); s.type = 'text/javascript'; s.async = true; s.src = url + '/widget-send-button/js/init.js';
            s.onload = function () { WhWidgetSendButton.init(host, '', options); };
            var x = document.getElementsByTagName('script')[0]; x.parentNode.insertBefore(s, x);

          
        })();
    </script>
    @await Html.PartialAsync("~/Views/Shared/Footer.cshtml")
    <script>
        $(document).ready(function () {
          
            $('#owl-carousel-0').owlCarousel({
                margin: 10,
                nav: true,
                loop: true,
                autoplay: true,
                autoplayTimeout: 2000,
                autoplayHoverPause: true,
                navText: ['<i class="fa fa-chevron-left"></i>', '<i class="fa fa-chevron-right"></i>'],
                responsive: {
                    0: {
                        items: 1
                    },
                    768: {
                        items: 2
                    },
                    1200: {
                        items: 3
                    }
                }
            });

            $('#owl-carousel-1').owlCarousel({
                margin: 10,
                nav: true,
                loop: true,
                autoplay: true,
                autoplayTimeout: 2000,
                autoplayHoverPause: true,
                navText: ['<i class="fa fa-chevron-left"></i>', '<i class="fa fa-chevron-right"></i>'],
                responsive: {
                    0: {
                        items: 1
                    },
                    768: {
                        items: 2
                    },
                    1200: {
                        items: 3
                    }
                }
            });

            $("#ex-1").select2({
                placeholder: "Types"
            });

            $("#ex-2").select2({
                placeholder: "Categories"
            });

            $("#ex-3").select2({
                placeholder: "Bedrooms"
            });

            $("#ex-4").select2({
                placeholder: "Area"
            });

        })
    </script>
    <script>
        window.onscroll = function () { myFunction() };
        var navbar = document.getElementById("navbar");
        var sticky = navbar.offsetTop;
        function myFunction() {
            if (window.pageYOffset >= sticky) {
                navbar.classList.add("sticky")
            } else {
                navbar.classList.remove("sticky");
            }
        }
    </script>

   <script>
       $(window).load(function () {
           setTimeout(function () {
               $('a').filter(function (index) { return $(this).text() === "GetButton"; }).hide();
           }, 2000)

         
       });

   </script>
</body>
</html>