                                <button type="button" class="btn btn-primary mb-1"> @string.Format("{0:0.00}", @item.Price) M AED</button>
                                <a asp-action="GetPropertyDetail" title="@item.PropertyName" asp-route-id="@item.Id" asp-controller="PropertyDetails"class="btn btn-primary mb-1">View Details</a>
                            </div>
                        </div>
                    </div>
                }


            </div>
        </div>
