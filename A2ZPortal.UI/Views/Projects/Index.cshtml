@model IEnumerable<A2ZPortal.Core.ViewModel.PropertyDetail.ProeprtyDetailByNameVm>
@{
    ViewData["Title"] = "Projects";
}
<div class="container-wrap">
    <div class="main-content container-boxed max offset">
        <div class="row">
            <div class="noo-main col-md-12 right-sidebar  noo-page" role="main">

                <div class="noo-vc-row row " style="">
                    <div class="noo-vc-col col-md-12 ">
                        <div class="recent-properties">
                            <div class="properties grid">
                                <div class="properties-header">
                                    <h1 class="page-title">Properties</h1>
                                </div>
                                <div class="noo-property-compare">
                                    <form style="display: none" method="POST" action="http://wp.nootheme.com/citilights/compare-listings/" class="submit-compare-612a036c0bbe8">
                                        <h3 class="title-compare">
                                            Compare properties
                                        </h3>
                                        <div class="list-compare"></div>
                                        <input type="submit" name="submit-compare" value="Compare" />
                                    </form>
                                </div>
                                <div class="properties-content">
                                    @foreach (var data in Model)
                                    {

                                        <article id="@data.Id" class="post-11917 noo_property type-noo_property status-publish has-post-thumbnail hentry property_category-single-family-home property_location-california property_sub_location-brooklyn property_sub_location-district-10 property_sub_location-district-11 property_sub_location-district-2 property_sub_location-district-3 property_sub_location-district-4 property_sub_location-district-5 property_sub_location-district-6 property_sub_location-district-7 property_sub_location-district-8 property_sub_location-district-9 property_sub_location-manhattan property_sub_location-manhattanville property_sub_location-queens property_sub_location-staten-island property_sub_location-the-bronx property_sub_location-upper-west-side property_status-sales has-featured">
                                            <div class="property-featured">
                                                <a class="content-thumb xx" asp-action="GetPropertyDetail" asp-controller="PropertyDetails" asp-route-id="@data.Id">
                                                    <img style="height:300px;width:370px;" src="@data.ImagePath" class="attachment-property-thumb size-property-thumb wp-post-image" alt="" loading="lazy" />
                                                </a>
                                                <span class="property-label sold">@data.CategoryName</span>
                                                <span class="property-category"><a asp-action="GetPropertyDetail" asp-controller="PropertyDetails" asp-route-id="@data.Id" rel="tag">@data.PropertyName</a></span>
                                                <a class="out-link" title="" asp-action="GetPropertyDetail" asp-controller="PropertyDetails" asp-route-id="@data.Id">
                                                    <i class="fa fa-link" aria-hidden="true"></i>
                                                </a>
                                            </div>
                                            <div class="property-wrap">
                                                <h2 class="property-title">
                                                    <a  asp-action="GetPropertyDetail" asp-controller="PropertyDetails" asp-route-id="@data.Id" title="@data.PropertyName">@data.PropertyName</a>
                                                </h2>
                                                <div class="property-excerpt">
                                                    <p>@data.PropertyDescription</p>
                                                </div>
                                                <div class="property-summary">
                                                    <div class="property-detail">
                                                        <div class="area"><span class="property-meta-icon" style="background-image: url(http://wp.nootheme.com/citilights/wp-content/uploads/2019/11/size-icon_ccd251439ef2512942b64d525ced79fe.png);"></span><span class="property-meta">@data.AreaCovered</span></div>
                                                        <div class="bedrooms"><span class="property-meta-icon" style="background-image: url(http://wp.nootheme.com/citilights/wp-content/uploads/2019/11/bedroom-icon_92be012355310366e0bf07c8a41e35fe.png);"></span><span class="property-meta">@data.BedRooms</span></div>
                                                        <div class="bathrooms"><span class="property-meta-icon" style="background-image: url(http://wp.nootheme.com/citilights/wp-content/uploads/2019/11/bathroom-icon_ffc5ba8874014ad0a7fcc5aa3b8acd57.png);"></span><span class="property-meta">@data.BathRooms</span></div>
                                                    </div> <div class="property-info style-1">
                                                        <div class="property-price">
                                                            <span> </span>
                                                        </div>

                                                        <div class="property-action">
                                                            <i title="Compare" data-user="0" data-id="@data.Id" data-action="compare" data-thumbnail="@data.ImagePath" data-div="submit-compare-612a036c0bbe8" class="property-action-button fa fa-plus compare-11917" aria-hidden="true"></i>
                                                            <a asp-action="AddToFavourite" asp-controller="CustomerTransaction" asp-route-propId="@data.Id">
                                                                <i title="Add to favorites" class="fa fa-heart-o" aria-hidden="true"></i>
                                                            </a>
                                                            
                                                        </div>
                                                    </div>
                                                    
                                                </div>
                                            </div>
                                        </article>


                                    }

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>

        </div>
    </div>

</div>